{
  "id": "cloud-sched-viz",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "karlkfi/cloud-sched-viz:latest",
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  },
  "cmd": "bin/www --api-type=marathon --api-host=marathon.mesos --api-port=8080",
  "instances": 1,
  "cpus": 0.25,
  "mem": 128.0
}